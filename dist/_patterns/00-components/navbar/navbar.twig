  {% block branding %}
    {% include "@_bootstrap/navbar/navbar-brand.twig" with {
      path: url,
      text: "Branding text",
      image: {
        src: "../../images/logo.png",
        alt: "logo",
        width: 90
      }
    } %}
  {% endblock %}

  {% block left %}
      <div class="mr-auto">
        {% include "@_bootstrap/nav/nav.twig" with {
          items: nav_items,
          utility_classes: ['navbar-nav']
        } %}
      </div>
  {% endblock %}

{% endembed %}
